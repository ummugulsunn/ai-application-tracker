{
  "name": "ai-application-tracker",
  "version": "1.0.0",
  "description": "AI-powered application tracking system for job seekers",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "quick-start": "npm run build && npm start",
    "desktop": "cd electron && npm install && npm start",
    "desktop-build": "npm run build && cd electron && npm run dist",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPatterns=lib/__tests__",
    "test:integration": "jest --testPathPatterns=integration",
    "test:accessibility": "jest --testPathPatterns=accessibility",
    "test:performance": "jest --testPathPatterns=performance",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:coverage && npm run test:e2e",
    "test:ci": "jest --coverage --watchAll=false && playwright test",
    "test:smoke": "playwright test --grep @smoke",
    "deploy": "./scripts/deploy.sh",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:production": "./scripts/deploy.sh production",
    "health-check": "curl -f http://localhost:3000/health || exit 1",
    "backup": "node -e \"require('./lib/disasterRecovery').disasterRecovery.performAutomatedBackup()\"",
    "monitor": "node -e \"console.log('Monitoring dashboard: http://localhost:3000/admin/monitoring')\""
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^12.0.0",
    "@headlessui/react": "^1.7.0",
    "@heroicons/react": "^2.0.0",
    "@hookform/resolvers": "^3.3.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.14.0",
    "@types/node": "^20.0.0",
    "@types/papaparse": "^5.3.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.0",
    "idb": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.2",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.292.0",
    "next": "^15.5.0",
    "next-auth": "^4.24.11",
    "next-swagger-doc": "^0.4.1",
    "openai": "^5.15.0",
    "papaparse": "^5.4.0",
    "postcss": "^8.4.0",
    "prisma": "^6.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.0",
    "react-query": "^3.39.0",
    "recharts": "^2.8.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.29.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "web-vitals": "^5.1.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/jspdf": "^1.3.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-react": "^5.18.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.0.0",
    "jest": "^30.0.5",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "lighthouse": "^11.4.0",
    "playwright": "^1.40.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "ts-jest": "^29.4.1"
  }
}
